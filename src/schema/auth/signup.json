{
  "title": "Signup API (회원가입)",
  "description": "velopick 회원가입 API",
  "summary": "회원가입",
  "tags": ["회원인증"],
  "body": {
    "type": "object",
    "required": ["email", "password", "username"],
    "properties": {
      "email": { "type": "string" },
      "password": { "type": "string" },
      "username": { "type": "string" },
      "gender": {
        "type": "string",
        "enum": ["M", "F"]
      },
      "birthday": { "type": "number" }
    }
  }
}
